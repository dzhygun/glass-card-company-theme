<div class="body-content-fade-in">
   <header class="top js-header glass-card">
      <a class="logo" href="{{@website.url}}">
         {{#if @website.logo}}
         <img src="{{@website.logo}}" alt="{{@website.name}}" width="{{@website.logoSize.width}}"
            height="{{@website.logoSize.height}}">
         {{else}}
         {{@website.name}}
         {{/if}}
      </a>
      {{#if menus.mainMenu}}
      {{> menu menus.mainMenu}}
      {{/if}}
   </header>